# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

##########################################
# File Extension Settings
##########################################

# Batch Files
[*.{cmd,bat}]
end_of_line = crlf

# YAML Files
[*.{yml,yaml}]
indent_size = 2
